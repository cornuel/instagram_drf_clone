{
	"name": "Django with Postgres and PgAdmin",
	// debug dockerComposeFile
	// ig-api-debug will overwrite ig-api
	"dockerComposeFile": ["../docker-compose.yml", "../docker-compose.debug.yml"],
	// regular dockerComposeFile
	// "dockerComposeFile": "../docker-compose.yml",
	"service": "django",
	"workspaceFolder": "/code",
	"forwardPorts": [8000],
	"customizations": {
		"vscode": {
			"extensions": [
				"ms-python.python",
				"ms-python.vscode-pylance",
				"ms-python.autopep8",
				"VisualStudioExptTeam.vscodeintellicode",
				"VisualStudioExptTeam.intellicode-api-usage-examples",
				"eamodio.gitlens",
				"Codeium.codeium",
				"batisteo.vscode-django",
				"vscode.github",
				"vscode.git",
				"vscode.git-base"
			]
		}
	}
}